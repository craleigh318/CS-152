(define union
  (lambda (lst1 lst2)
    (cond
      ((null? lst1)
       lst2)
      ((null? lst2)
       lst1)
      (else
        (if (not (member (car lst2) lst1))
            (union (append lst1 (car lst2)) (cdr lst2))
            (union lst1 lst2))))))